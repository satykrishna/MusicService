            $ref: "#/definitions/Song"
        "400":
          description: "Invalid ID supplied"
        "404":
          description: "No Song found"
    delete:
      tags:
      - "song"
      summary: "Deletes a Song By Song ID"
      description: "Delete Song By Song Id"
      operationId: "deleteSong"
      produces:
      - "application/json"
      parameters:
      - name: "song_id"
        in: "path"
        description: "ID of Song to return"
        required: true
